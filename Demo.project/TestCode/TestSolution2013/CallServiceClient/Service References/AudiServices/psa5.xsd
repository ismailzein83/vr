    <xsd:sequence>
      <xsd:element minOccurs="0" name="instalmentType" type="xsd:string" />
      <xsd:element minOccurs="0" name="amount" type="bsc:FinancialAmountType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="instalment" type="InstalmentType" />
  <xsd:complexType name="BillType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="billAmount" type="bsc:FinancialAmountType">
        <xsd:annotation>
          <xsd:documentation>Indicates the transaction amount within the billing life cylce</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="dueDate" type="xsd:date" />
      <xsd:element minOccurs="0" name="minimumAmountDue" type="bsc:FinancialAmountType">
        <xsd:annotation>
          <xsd:documentation>Indicates the minimum amount due to be settled within the same billing cycle</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="minimumPercentDue" type="bsc:PercentType" />
      <xsd:element minOccurs="0" name="currentOutstandingBalance" type="bsc:FinancialAmountType">
        <xsd:annotation>
          <xsd:documentation>Indicates the outstanding balance  </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="billCycle" type="xsd:unsignedInt">
        <xsd:annotation>
          <xsd:documentation>Indicates the Number of days before Applying the rate on the bill amount</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="totalUnbilledAmount" type="bsc:FinancialAmountType" />
      <xsd:element minOccurs="0" name="instalmentAmount" type="bsc:FinancialAmountType" />
      <xsd:element minOccurs="0" name="autoPayment" type="xsd:boolean" />
      <xsd:element minOccurs="0" name="billCurrency" type="bsc:ISO4217CurrencyType" />
      <xsd:element minOccurs="0" name="totalBalance" type="bsc:FinancialAmountType" />
      <xsd:element minOccurs="0" name="paidBillAmount" type="bsc:FinancialAmountType" />
      <xsd:element minOccurs="0" name="remainingMinimumAmountDue" type="bsc:FinancialAmountType" />
      <xsd:element minOccurs="0" name="previousMinimumAmountDue" type="bsc:FinancialAmountType" />
      <xsd:element minOccurs="0" name="totalAmountDue" type="bsc:FinancialAmountType" />
      <xsd:element minOccurs="0" ref="billingAddress" />
      <xsd:element minOccurs="0" name="totalAmountPaid" type="bsc:FinancialAmountType" />
      <xsd:element minOccurs="0" name="lastDueDate" type="xsd:date" />
      <xsd:element minOccurs="0" name="pastDueAmount" type="bsc:FinancialAmountType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="bill" type="BillType" />
