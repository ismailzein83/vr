          <xsd:element minOccurs="0" name="shadowAccount" type="CardAccountType" />
          <xsd:element minOccurs="0" name="availableFacility" type="bsc:FinancialAmountType" />
          <xsd:element minOccurs="0" ref="bill" />
          <xsd:element minOccurs="0" name="cashRate" type="bsc:PercentType" />
          <xsd:element minOccurs="0" name="purchaseRate" type="bsc:PercentType" />
          <xsd:element minOccurs="0" name="isDelinquent" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Indicates if the customer is late on his card payments</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="processDate" type="xsd:date" />
          <xsd:element minOccurs="0" name="frozenAmount" type="bsc:FinancialAmountType" />
          <xsd:element minOccurs="0" ref="instalment" />
          <xsd:element minOccurs="0" name="numberOfRemainingInsalments" type="xsd:string" />
        </xsd:sequence>
      </xsd:extension>
