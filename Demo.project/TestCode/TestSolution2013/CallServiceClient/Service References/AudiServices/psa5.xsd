    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" ref="cardLimit" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="cardLimitSet" type="CardLimitSetType" />
