    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="deliveryStatus" type="DeliveryStatusType" />
  <xsd:complexType name="DispatchType">
    <xsd:sequence>
      <xsd:element minOccurs="0" ref="dispatchItem" />
      <xsd:element minOccurs="0" name="date" type="xsd:date" />
      <xsd:element minOccurs="0" ref="deliveryType" />
      <xsd:element minOccurs="0" ref="deliveryAddress" />
      <xsd:element minOccurs="0" ref="deliveryStatus" />
      <xsd:element minOccurs="0" name="deliveryPeriod" type="xsd:string" />
      <xsd:element minOccurs="0" name="deliveryBranch" type="brn:BranchSummaryType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="dispatch" type="DispatchType" />
  <xsd:complexType name="DispatchSetType">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" ref="dispatch" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="dispatchSet" type="DispatchSetType" />
  <xsd:complexType name="ChannelSetType">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" name="channel" type="xsd:string" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="channelSet" type="ChannelSetType" />
