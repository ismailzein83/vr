      <xsd:extension base="CardAccountType">
        <xsd:sequence>
          <xsd:element ref="acc:status" />
          <xsd:element name="description" type="xsd:string" />
          <xsd:element name="bookBalance" type="bsc:AccountingMoneyType" />
          <xsd:element name="availableBalance" type="bsc:AccountingMoneyType" />
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
