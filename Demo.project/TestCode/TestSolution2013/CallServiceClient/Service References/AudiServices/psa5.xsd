    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" ref="prepaidCardAccount" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="prepaidCardAccountList" type="PrepaidCardAccountListType" />
