    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" ref="feature" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="featureSet" type="FeatureSetType" />
  <xsd:complexType name="ProductType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="productID" type="xsd:string" />
      <xsd:element minOccurs="0" name="name" type="xsd:string" />
