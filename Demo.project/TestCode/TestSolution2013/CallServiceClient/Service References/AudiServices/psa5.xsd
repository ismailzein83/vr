    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="MagneticTrackType">
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="magneticTrackFormat" type="MagneticTrackFormatType" use="required" />
        <xsd:attribute name="isEncrypted" type="bsc:YNType" use="required" />
        <xsd:attribute name="encryptionKeyId" type="xsd:string" use="optional" />
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:element name="magneticTrack" type="MagneticTrackType" />
  <xsd:complexType name="MagneticTrackSetType">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" ref="magneticTrack" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="magneticTrackSet" type="MagneticTrackSetType" />
  <xsd:complexType name="CardAccountType">
    <xsd:sequence>
      <xsd:element minOccurs="0" ref="id:accountId" />
      <xsd:element minOccurs="0" name="currency" type="bsc:ISO4217CurrencyType" />
      <xsd:element minOccurs="0" name="formattedAccountId" type="id:AccountIdType" />
      <xsd:element minOccurs="0" name="propertySet" type="bsc:PropertySetType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="cardAccount" type="CardAccountType" />
  <xsd:complexType name="CardAccountListType">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" ref="cardAccount" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="cardAccountList" type="CardAccountListType" />
  <xsd:complexType name="CardOperationType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="operationCode" type="xsd:string" />
      <xsd:element minOccurs="0" name="operationName" type="xsd:string" />
    </xsd:sequence>
    <xsd:attribute name="allowFX" type="xsd:string" use="required" />
  </xsd:complexType>
  <xsd:element name="cardOperation" type="CardOperationType" />
  <xsd:complexType name="AllowedCardOperationsSetType">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" ref="cardOperation" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="allowedCardOperationsSet" type="AllowedCardOperationsSetType" />
