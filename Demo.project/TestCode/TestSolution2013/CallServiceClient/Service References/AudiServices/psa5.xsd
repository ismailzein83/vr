    <xsd:sequence>
      <xsd:element ref="cardAccountList" />
      <xsd:element minOccurs="0" maxOccurs="1" ref="allowedCardOperationsSet" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="debitCardDetail" type="DebitCardDetailType" />
