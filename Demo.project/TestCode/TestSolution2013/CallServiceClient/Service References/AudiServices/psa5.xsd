      <xsd:element minOccurs="0" name="stopDate" type="xsd:date" />
      <xsd:element minOccurs="0" name="startDate" type="xsd:date" />
      <xsd:element minOccurs="0" ref="deliveryType" />
      <xsd:element minOccurs="0" ref="deliveryAddress">
        <xsd:annotation>
          <xsd:documentation>Based on the delivery Type, the Address will be chosen </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="notificationSubscription" type="NotificationSubscriptionType" />
  <xsd:complexType name="NotificationSubscriptionSetType">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" ref="notificationSubscription" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="notificationSubscriptionSet" type="NotificationSubscriptionSetType" />
  <xsd:simpleType name="CardIssuanceTypeType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Primary" />
      <xsd:enumeration value="Supplementary" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="cardIssuanceType" type="CardIssuanceTypeType" />
