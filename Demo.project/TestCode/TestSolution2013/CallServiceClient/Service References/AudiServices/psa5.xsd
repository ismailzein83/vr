        <xsd:sequence>
          <xsd:element minOccurs="0" ref="channelSet" />
          <xsd:element minOccurs="0" name="limitCount" type="xsd:unsignedInt" />
          <xsd:element minOccurs="0" name="limitCountUtilisation" type="xsd:unsignedInt" />
          <xsd:element minOccurs="0" name="limitCurrency" type="bsc:ISO4217CurrencyType" />
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="cardLimit" type="CardLimitType" />
