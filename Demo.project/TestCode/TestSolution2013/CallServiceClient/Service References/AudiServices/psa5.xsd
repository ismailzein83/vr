  <xsd:simpleType name="StatusTypeType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Active" />
      <xsd:enumeration value="Blocked" />
      <xsd:enumeration value="Closed" />
      <xsd:enumeration value="Expired" />
      <xsd:enumeration value="Issued" />
      <xsd:enumeration value="HotListed" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="CardStatusSetType">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" ref="cardStatus" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="cardStatusSet" type="CardStatusSetType" />
  <xsd:complexType name="CardStatusType">
    <xsd:sequence>
      <xsd:element name="status" type="StatusTypeType" />
      <xsd:element minOccurs="0" name="subStatus" type="xsd:string" />
      <xsd:element minOccurs="0" name="description" type="xsd:string" />
      <xsd:element minOccurs="0" name="dateOfLastStatusChange" type="xsd:date" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="cardStatus" type="CardStatusType" />
  <xsd:simpleType name="DeliveryTypeType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Email" />
      <xsd:enumeration value="Mail" />
      <xsd:enumeration value="SMS" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="deliveryType" type="DeliveryTypeType" />
  <xsd:complexType name="DeliveryAddressType">
    <xsd:choice>
      <xsd:element minOccurs="0" ref="condet:addressContact" />
      <xsd:element minOccurs="0" ref="condet:emailAddressContact" />
      <xsd:element minOccurs="0" ref="condet:phoneContact" />
    </xsd:choice>
  </xsd:complexType>
  <xsd:element name="deliveryAddress" type="DeliveryAddressType" />
  <xsd:complexType name="BillingAddressType">
    <xsd:sequence>
      <xsd:element minOccurs="0" ref="condet:addressContactSet" />
      <xsd:element minOccurs="0" ref="condet:emailAddressContactSet" />
      <xsd:element minOccurs="0" ref="condet:phoneContactSet" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="billingAddress" type="BillingAddressType" />
  <xsd:complexType name="ResidentialAddressType">
    <xsd:sequence>
      <xsd:element minOccurs="0" ref="condet:addressContactSet" />
      <xsd:element minOccurs="0" ref="condet:emailAddressContactSet" />
      <xsd:element minOccurs="0" ref="condet:phoneContactSet" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="residentialAddress" type="ResidentialAddressType" />
  <xsd:simpleType name="NotificationSubscriptionStatusType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Active" />
      <xsd:enumeration value="Inactive" />
      <xsd:enumeration value="DeclinedByCustomer" />
      <xsd:enumeration value="Unknown" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="NotificationSubscriptionType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="fee" type="bsc:FinancialAmountType" />
