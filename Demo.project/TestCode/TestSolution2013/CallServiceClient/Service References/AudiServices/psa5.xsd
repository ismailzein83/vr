    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="PIN Block ISO 9564 Format 0" />
      <xsd:enumeration value="PIN Block ISO 9564 Format 1" />
      <xsd:enumeration value="PIN Block ISO 9564 Format 3" />
      <xsd:enumeration value="Clear" />
      <xsd:enumeration value="Unknown" />
      <xsd:enumeration value="Other" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="PinType">
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="pinFormat" type="PinFormatType" use="required" />
        <xsd:attribute name="isEncrypted" type="bsc:YNType" use="required" />
        <xsd:attribute name="encryptionKeyId" type="xsd:string" use="optional" />
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:element name="pin" type="PinType" />
  <xsd:complexType name="CardNumberType">
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="isEncrypted" type="bsc:YNType" use="required" />
        <xsd:attribute name="encryptionKeyId" type="xsd:string" use="optional" />
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:element name="cardNumber" type="CardNumberType" />
  <xsd:complexType name="CardNumberCollectionType">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" ref="cardNumber" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="cardNumberSet" type="CardNumberCollectionType" />
  <xsd:element name="cardNumberList" type="CardNumberCollectionType" />
