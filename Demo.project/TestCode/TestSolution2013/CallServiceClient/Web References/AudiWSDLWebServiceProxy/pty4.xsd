    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Elementary" />
      <xsd:enumeration value="Intermediary" />
      <xsd:enumeration value="Secondary" />
      <xsd:enumeration value="Baccalaureat" />
      <xsd:enumeration value="Diploma" />
      <xsd:enumeration value="None" />
      <xsd:enumeration value="Postgraduate" />
      <xsd:enumeration value="Bachelor/Licence" />
      <xsd:enumeration value="Masters/Maitrise" />
      <xsd:enumeration value="Post Masters/DES" />
      <xsd:enumeration value="PhD/MD" />
      <xsd:enumeration value="Illiterate" />
      <xsd:enumeration value="Technical School" />
      <xsd:enumeration value="Banking Studies Diploma" />
      <xsd:enumeration value="Bachelor of Engineering" />
      <xsd:enumeration value="Medicine Doctorat" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="EducationDetailsType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="educationLevel" type="EducationLevelType" />
      <xsd:element minOccurs="0" name="institutionName" type="xsd:string" />
