          <xsd:element minOccurs="0" name="age" type="xsd:unsignedInt" />
          <xsd:element minOccurs="0" name="gender" type="bsc:GenderType" />
          <xsd:element minOccurs="0" name="dateOfBirth" type="xsd:date" />
          <xsd:element minOccurs="0" name="dateOfDeath" type="xsd:date" />
          <xsd:element minOccurs="0" ref="nationalitySet" />
          <xsd:element minOccurs="0" ref="residency" />
          <xsd:element minOccurs="0" ref="employmentDetailsSet" />
          <xsd:element minOccurs="0" name="personSummaryDetails" type="PersonalDetailsType" />
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="personSummary" type="PersonSummaryType" />
