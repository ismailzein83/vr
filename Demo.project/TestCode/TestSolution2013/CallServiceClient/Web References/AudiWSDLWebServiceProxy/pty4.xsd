      <xsd:element minOccurs="0" name="clientRiskScore" type="xsd:string" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="FraudScoreType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Clear" />
      <xsd:enumeration value="Blocked" />
      <xsd:enumeration value="Partial" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="PersonType">
    <xsd:complexContent mixed="false">
      <xsd:extension base="PersonSummaryType">
        <xsd:sequence>
          <xsd:element minOccurs="0" ref="languageSet" />
