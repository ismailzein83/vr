    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Married" />
      <xsd:enumeration value="Re-Married" />
      <xsd:enumeration value="Defacto" />
      <xsd:enumeration value="Single" />
      <xsd:enumeration value="Divorced" />
      <xsd:enumeration value="Separated" />
      <xsd:enumeration value="Spouse Expired" />
      <xsd:enumeration value="Widow" />
      <xsd:enumeration value="Widower" />
    </xsd:restriction>
  </xsd:simpleType>
