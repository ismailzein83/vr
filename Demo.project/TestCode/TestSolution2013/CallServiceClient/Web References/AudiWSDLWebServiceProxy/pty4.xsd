  <xsd:complexType name="EmploymentDetailsSetType">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" ref="employmentDetails" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="employmentDetailsSet" type="EmploymentDetailsSetType" />
