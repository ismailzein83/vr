      <xsd:element minOccurs="0" name="civilRegistryCity" type="xsd:string" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="maritalDetails" type="MaritalDetailsType" />
  <xsd:complexType name="ReligionDetailsType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="religion" type="xsd:string" />
      <xsd:element minOccurs="0" name="description" type="xsd:string" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="religionDetails" type="ReligionDetailsType" />
  <xsd:complexType name="DependentType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="name" type="xsd:string" />
      <xsd:element minOccurs="0" name="dateOfBirth" type="xsd:date" />
