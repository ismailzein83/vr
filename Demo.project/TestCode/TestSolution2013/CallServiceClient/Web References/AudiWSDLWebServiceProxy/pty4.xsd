    <xsd:sequence>
      <xsd:element minOccurs="0" name="country" type="xsd:string" />
      <xsd:element minOccurs="0" name="dateOfAttainment" type="xsd:date" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="nationality" type="NationalityType" />
  <xsd:complexType name="NationalitySetType">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" ref="nationality" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="nationalitySet" type="NationalitySetType" />
