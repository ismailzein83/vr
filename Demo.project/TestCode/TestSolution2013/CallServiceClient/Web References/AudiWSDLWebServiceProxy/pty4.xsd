  <xsd:complexType name="EmploymentStatusTypeType">
    <xsd:simpleContent>
      <xsd:extension base="EmploymentStatusType">
        <xsd:attribute name="description" use="optional" />
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
