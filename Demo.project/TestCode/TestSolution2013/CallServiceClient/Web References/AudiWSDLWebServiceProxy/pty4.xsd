  <xsd:complexType name="ChannelRetailUserType">
    <xsd:complexContent mixed="false">
      <xsd:extension base="PersonType">
        <xsd:sequence>
          <xsd:element ref="cust:userDetails" />
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="channelRetailUser" type="ChannelRetailUserType" />
</xsd:schema>