    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="At Home" />
      <xsd:enumeration value="Unemployed" />
      <xsd:enumeration value="Self-Employed" />
      <xsd:enumeration value="Full-time Permanent" />
      <xsd:enumeration value="Full-time Temporary" />
      <xsd:enumeration value="Part-time" />
      <xsd:enumeration value="Student" />
      <xsd:enumeration value="Casual" />
      <xsd:enumeration value="Retired - Pension" />
      <xsd:enumeration value="Retired - Non-Pensioned" />
      <xsd:enumeration value="Other Government Benefit" />
      <xsd:enumeration value="Other" />
      <xsd:enumeration value="Business Owner of less than 5 Employees" />
      <xsd:enumeration value="Business Owner of less than 10 Employees" />
      <xsd:enumeration value="Business Owner of greater than or equal 10 Employees" />
      <xsd:enumeration value="Independent" />
      <xsd:enumeration value="Corporation Owner" />
      <xsd:enumeration value="Establishment Owner" />
      <xsd:enumeration value="Corporation Owner and Establishment Owner" />
      <xsd:enumeration value="Full-time Permanent and Establishment Owner and Corporation Owner" />
      <xsd:enumeration value="Full-time Permanent and Establishment Owner" />
      <xsd:enumeration value="Full-time Permanent and Corporation Owner" />
      <xsd:enumeration value="Self-Employed and Establishment Owner and Corporation Owner" />
      <xsd:enumeration value="Self-Employed and Establishment Owner" />
      <xsd:enumeration value="Self-Employed and Corporation Owner" />
      <xsd:enumeration value="Independent and Establishment Owner and Corporation Owner" />
      <xsd:enumeration value="Independent and Establishment Owner" />
      <xsd:enumeration value="Independent and Corporation Owner" />
      <xsd:enumeration value="Not Available" />
