  <xsd:import schemaLocation="pty.org.Organisation_1.0.xsd" namespace="http://www.banqueaudi.com/ebo/pty.org.Organisation" />
  <xsd:import schemaLocation="pty.cust.Customer_1.0.xsd" namespace="http://www.banqueaudi.com/ebo/pty.cust.Customer" />
  <xsd:import schemaLocation="psa.cmn.Identifiers_1.0.xsd" namespace="http://www.banqueaudi.com/ebo/psa.cmn.Identifiers" />
  <xsd:complexType name="RelationshipType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="type" type="xsd:string" />
      <xsd:element minOccurs="0" name="description" type="xsd:string" />
      <xsd:element minOccurs="0" name="others" type="xsd:string" />
      <xsd:element minOccurs="0" name="sequence" type="xsd:unsignedInt" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="relationship" type="RelationshipType" />
  <xsd:complexType name="NameDetailType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="salutation" type="xsd:string" />
      <xsd:element minOccurs="0" name="firstName" type="xsd:string" />
      <xsd:element minOccurs="0" name="middleName" type="xsd:string" />
      <xsd:element minOccurs="0" name="lastName" type="xsd:string" />
      <xsd:element minOccurs="0" name="fathersName" type="xsd:string" />
      <xsd:element minOccurs="0" name="mothersName" type="xsd:string" />
      <xsd:element minOccurs="0" name="birthLastName" type="xsd:string" />
      <xsd:element minOccurs="0" name="spouseLastName" type="xsd:string" />
      <xsd:element minOccurs="0" name="nickname" type="xsd:string" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="nameDetail" type="NameDetailType" />
  <xsd:complexType name="NameType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="shortName" type="xsd:string" />
      <xsd:element minOccurs="0" name="longName" type="xsd:string" />
      <xsd:element minOccurs="0" name="effectiveDate" type="xsd:date" />
      <xsd:element minOccurs="0" ref="nameDetail" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="name" type="NameType" />
  <xsd:simpleType name="LanguageAbilityType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Spoken" />
      <xsd:enumeration value="Written" />
      <xsd:enumeration value="Spoken and Written" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="LanguageType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="language" type="bsc:IETFLanguageCodeType" />
      <xsd:element minOccurs="0" name="description" type="xsd:string" />
      <xsd:element minOccurs="0" name="ability" type="LanguageAbilityType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="language" type="LanguageType" />
  <xsd:complexType name="LanguageSetType">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" ref="language" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="languageSet" type="LanguageSetType" />
