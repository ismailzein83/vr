<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="vanriseLogging" type="Vanrise.Common.LoggingConfiguration.LoggingConfig, Vanrise.Common" />
    <section name="vanriseRuntime" type="Vanrise.Runtime.Configuration.RuntimeConfig, Vanrise.Runtime" />
  </configSections>
  <!-- Vanrise Runtime is configured as per client resources (mainly CPU \ RAM Memory) -->
  <vanriseRuntime>
    <runtimeServiceGroups>
      <!-- Used for scheduling services for data sources, workflow. nbOfRuntimeInstances recommended to be equal to 1 or 2.-->
      <runtimeServiceGroup name="group2" nbOfRuntimeInstances="1">
        <runtimeServices>
          <runtimeService name="Scheduler Service" type="Vanrise.Runtime.SchedulerService, Vanrise.Runtime" interval="00:00:02"></runtimeService>
        </runtimeServices>
      </runtimeServiceGroup>
      <!-- Caching Distributor service manages Caching Runtime services. nbOfRuntimeInstances Keep it 1-->
      <runtimeServiceGroup name="groupA1" nbOfRuntimeInstances="1">
        <runtimeServices>
          <runtimeService name="CachingDistributorRuntimeService" type="Vanrise.Caching.Runtime.CachingDistributorRuntimeService, Vanrise.Caching.Runtime" interval="00:00:01"></runtimeService>
        </runtimeServices>
      </runtimeServiceGroup>
      <!-- Caching Runtime service used as centralized memory cache for business entities. nbOfRuntimeInstances depended on available resources-->
      <runtimeServiceGroup name="groupA2" nbOfRuntimeInstances="2">
        <runtimeServices>
          <runtimeService name="CachingRuntimeService" type="Vanrise.Caching.Runtime.CachingRuntimeService,Vanrise.Caching.Runtime" interval="00:00:01"></runtimeService>
        </runtimeServices>
      </runtimeServiceGroup>
    </runtimeServiceGroups>
  </vanriseRuntime>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <vanriseLogging maxLogLevel="Information">
    <loggers>
      <logger name="SQLLogger" type="Vanrise.Logging.SQL.SQLLogger, Vanrise.Logging.SQL">
        <parameters>
          <parameter name="ConnectionStringKey" value="LogDBConnString" />
        </parameters>
      </logger>
      <logger name="ConsoleLogger" type="Vanrise.Common.ConsoleLogger, Vanrise.Common">
      </logger>
    </loggers>
    <exceptionLoggers>
      <exceptionLogger name="ConsoleExceptionLogger" type="Vanrise.Common.ConsoleExceptionLogger, Vanrise.Common">
      </exceptionLogger>
      <exceptionLogger name="DefaultExceptionLogger" type="Vanrise.Common.DefaultExceptionLogger, Vanrise.Common">
      </exceptionLogger>
    </exceptionLoggers>
  </vanriseLogging>
  <connectionStrings>
