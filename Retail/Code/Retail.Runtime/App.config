<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="vanriseRuntime" type="Vanrise.Runtime.Configuration.RuntimeConfig, Vanrise.Runtime" />
    <section name="vanriseLogging" type="Vanrise.Common.LoggingConfiguration.LoggingConfig, Vanrise.Common" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>

  <vanriseRuntime>
    <runtimeServiceGroups>
      <runtimeServiceGroup name="group1" nbOfRuntimeInstances="15">
        <runtimeServices>
          <runtimeService name="Business Process Service" type="Vanrise.BusinessProcess.BusinessProcessService, Vanrise.BusinessProcess" interval="00:00:02"></runtimeService>
        </runtimeServices>
      </runtimeServiceGroup>
      <!--<runtimeServiceGroup name="group2" nbOfRuntimeInstances="1">
        <runtimeServices>
          <runtimeService name="Scheduler Service" type="Vanrise.Runtime.SchedulerService, Vanrise.Runtime" interval="00:00:02"></runtimeService>
        </runtimeServices>
      </runtimeServiceGroup>
      <runtimeServiceGroup name="group3" nbOfRuntimeInstances="15">
        <runtimeServices>
          <runtimeService name="Queue Activation Service" type="Vanrise.Queueing.QueueActivationService, Vanrise.Queueing" interval="00:00:02"></runtimeService>
        </runtimeServices>
      </runtimeServiceGroup>
      <runtimeServiceGroup name="group4" nbOfRuntimeInstances="12">
        <runtimeServices>
          <runtimeService name="Data Source Runtime Service" type="Vanrise.Integration.Business.DataSourceRuntimeService, Vanrise.Integration.Business" interval="00:00:02"></runtimeService>
        </runtimeServices>
      </runtimeServiceGroup>-->
    </runtimeServiceGroups>
  </vanriseRuntime>

  <connectionStrings>
    <add name="ConfigurationDBConnString" connectionString="Server=192.168.110.185;Database=Retail_Dev_Configuration;User ID=development;Password=dev!123;" />
    <add name="RetailDBConnString" connectionString="Data Source=192.168.110.185;Initial Catalog=Retail_Dev;User ID=development;Password=dev!123;" providerName="System.Data.SqlClient" />
    <add name="MainDBConnString" connectionString="Server=192.168.110.185;User ID=development;Password=dev!123;Database=Retail_Dev;"/>
    <add name="LogDBConnString" connectionString="Initial Catalog=Retail_Dev_Logging;Data Source=.;User ID=sa; Password=p@ssw0rd" />
    <add name="TransactionDBConnString" connectionString="Data Source=.;Initial Catalog=Retail_Dev_Transaction;User ID=sa; Password=p@ssw0rd" />
    <add name="QueueItemDBConnString" connectionString="Data Source=.;Initial Catalog=Retail_Dev_Queue;User ID=sa; Password=p@ssw0rd" />
    <add name="RetailCDRDBConnString" connectionString="Server=192.168.110.185;Database=Retail_Dev_CDR;User ID=Development;Password=dev!123"/>
  </connectionStrings>

  <appSettings>
    <add key="Retail_BE_DBConnStringKey" value="RetailDBConnString" />
    <add key="SecurityDBConnStringKey" value="ConfigurationDBConnString" />
    <add key="ConfigurationDBConnStringKey" value="ConfigurationDBConnString" />
    <add key="RuntimeConnStringKey" value="TransactionDBConnString" />
    <add key="BusinessProcessDBConnStringKey" value="TransactionDBConnString" />
    <add key="BusinessProcessTrackingDBConnStringKey" value="LogDBConnString" />
    <add key="BusinessProcessConfigDBConnStringKey" value="ConfigurationDBConnString" />
    <add key="IntegrationConnStringKey" value="TransactionDBConnString"/>
    <add key="QueueItemDBConnStringKey" value="QueueItemDBConnString"/>
    <add key="QueueDBConnStringKey" value="TransactionDBConnString"/>
    <add key="ExposedConnectionStringNames" value="RetailCDRDBConnString"/>

    <add key="IsRuntimeService" value="false"/>
  </appSettings>

  <vanriseLogging maxLogLevel="Information">
    <loggers>
      <logger name="SQLLogger" type="Vanrise.Logging.SQL.SQLLogger, Vanrise.Logging.SQL">
        <parameters>
          <parameter name="ConnectionStringKey" value="LogDBConnString" />
        </parameters>
      </logger>
      <logger name="ConsoleLogger"  type="Vanrise.Common.ConsoleLogger, Vanrise.Common"></logger>
    </loggers>
    <exceptionLoggers>
      <exceptionLogger name="ConsoleExceptionLogger"  type="Vanrise.Common.ConsoleExceptionLogger, Vanrise.Common"></exceptionLogger>
      <exceptionLogger name="DefaultExceptionLogger"  type="Vanrise.Common.DefaultExceptionLogger, Vanrise.Common"></exceptionLogger>
    </exceptionLoggers>
  </vanriseLogging>

</configuration>