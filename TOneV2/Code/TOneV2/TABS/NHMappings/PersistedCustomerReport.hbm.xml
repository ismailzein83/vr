<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
	<class name="PersistedCustomReport" table="PersistedCustomReport" dynamic-update="false" lazy="false">
		<cache usage="nonstrict-read-write" />
		<id name="ID" column="ID">
			<generator class="identity" />
		</id>
		<property name="Name" column="[Name]" />
    <property name="Created" column="Created" />
    <property name="Updated" column="Updated" />
    <property name="Csharp_Code" column="Csharp_Code" type="StringClob" access="property" />
    <property name="IsEncrypted" column="IsEncrypted" type="YesNo" />
    <many-to-one name="RequiredPermission" column="RequiredPermissionID" fetch="select" />
    <many-to-one name="User" column="UserID" fetch="select" />
  </class>
</hibernate-mapping>