<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
  <class name="PricingTemplatePlan" table="PricingTemplatePlan" dynamic-update="false" lazy="false">
    <cache usage="nonstrict-read-write"/>
    <id name="PricingTemplatePlanId" column="PricingTemplatePlanId" unsaved-value="0" >
      <generator class="identity" />
    </id>
    <many-to-one name="PricingTemplate" column="PricingTemplateId" />
    <many-to-one name="Zone" column="ZoneID" fetch="select" />
    <property name="ServicesFlag" column="ServicesFlag" />
    <property name="FromPrice" column="FromPrice" />
    <property name="ToPrice" column="ToPrice" />
    <property name="Margin" column="Margin" />
    <property name="MaxMargin" column="MaxMargin"/>
    <property name="Fixed" column="Fixed" />
    <property name="IsPerc" column="IsPerc" type="YesNo" />
    <property name="PricingReferenceRate" column="PricingReferenceRate"/>
    <property name="NotContinues" column="NotContinues" type="YesNo" />
    <property name="Priority" column="Priority"/>
    <property name="Description" column="Description" />
    <many-to-one name="Supplier" column="SupplierID" fetch="select" />
  </class>  
</hibernate-mapping>
