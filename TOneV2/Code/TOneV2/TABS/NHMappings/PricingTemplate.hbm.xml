<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">

  <class name="PricingTemplate" table="PricingTemplate" dynamic-update="false" lazy="false">
    <cache usage="nonstrict-read-write"/>
    
    <id name="PricingTemplateId" column="PricingTemplateId" unsaved-value="0" >
      <generator class="identity" />
    </id>
    <property name="Title" column="Title" />
    <many-to-one name="Currency" column="CurrencyID" fetch="select" />
    <property name="TemplateType" column="TemplateType" />
    <property name="Description" column="Description" />
  </class>
</hibernate-mapping>


