<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
	<class name="StateBackupInfo" table="StateBackup" dynamic-update="false" lazy="false"  polymorphism="explicit">
		<!-- <cache usage="nonstrict-read-write"/> -->
		<id name="ID" column="ID" type="Int32" unsaved-value="0">
			<generator class="identity" />
		</id>
    <property name="StateBackupType" column="StateBackupType" />
    <many-to-one name="Supplier" column="SupplierID" fetch="select" />
    <many-to-one name="Customer" column="CustomerID" fetch="select" />
    <property name="Created" column="Created" type="DateTime" />
    <property name="RestoreDate" column="RestoreDate" />
    <property name="Notes" column="Notes" />
    <property name="ResponsibleForRestoring" column="ResponsibleForRestoring" />
    <many-to-one name="User" column="UserID" fetch="select" />
	</class>
</hibernate-mapping>