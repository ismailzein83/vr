<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
	<class name="CodeGroup" table="CodeGroup" dynamic-update="false" lazy="false" >
		<cache usage="nonstrict-read-write"/>
		<id name="Code" column="Code" type="String">
			<generator class="assigned"/>
		</id>
		<property name="Name" column="[Name]" />
		<many-to-one name="User" column="UserID" fetch="select" />
	</class>
</hibernate-mapping>