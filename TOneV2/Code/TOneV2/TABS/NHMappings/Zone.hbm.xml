<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
	<class name="Zone" table="Zone" dynamic-update="false" lazy="false" >
		<cache usage="nonstrict-read-write" />
		<id name="_ZoneID" column="ZoneID" type="Int32" unsaved-value="0" access="field">
			<generator class="identity" />
		</id>
		<property name="Name" column="Name" type="String" length="255" />
		<many-to-one name="Supplier" column="SupplierID" fetch="select" />
		<many-to-one name="CodeGroup" column="CodeGroup" fetch="select" />
		<property name="ServicesFlag" column="ServicesFlag" />
		<property name="IsMobile" column="IsMobile" type="YesNo" />
		<property name="IsProper" column="IsProper" type="YesNo" />
		<property name="IsSold" column="IsSold" type="YesNo" />
		<property name="BeginEffectiveDate" column="BeginEffectiveDate" type="DateTime" />
		<property name="EndEffectiveDate" column="EndEffectiveDate" type="DateTime" />
		<many-to-one name="User" column="UserID" fetch="select" />    
	</class>
</hibernate-mapping>