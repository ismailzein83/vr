<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
	<class name="PersistedRunnableTask" table="PersistedRunnableTask" dynamic-update="false" lazy="false">
		<cache usage="nonstrict-read-write" />
		<id name="ID" column="ID">
			<generator class="identity" />
		</id>
		<property name="ScheduleType" column="[ScheduleType]" />
		<property name="Name" column="[Name]" />
		<property name="IsEnabled" column="[IsEnabled]" type="YesNo" />
		<property name="TimeSpan" column="[TimeSpan]" />
		<property name="_LastRun" column="[LastRun]" access="field" />
		<property name="_LastRunDuration" column="[LastRunDuration]" access="field" />
		<property name="_NextRun" column="[NextRun]" access="field" />
		<property name="ConfigurationXmlString" column="[Configuration]" type="StringClob" access="property" />
    <property name="GroupingExpression" column="GroupingExpression" />    
    <property name="IsLastRunSuccessful" column="IsLastRunSuccessful" type="YesNo" />
		<property name="ExceptionString" column="ExceptionString" type="StringClob" access="property" />
    <property name="IsService" column="IsService" type="YesNo" />
    <property name="ServiceStatus" column="[ServiceStatus]" />
	</class>
</hibernate-mapping>