<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
  <class name="Billing_Invoice_Detail" table="Billing_Invoice_Details" >
    <id name="ID"  column="[ID]">
      <generator class="identity"/>
    </id>
    <property name="InvoiceID" column="InvoiceID"  />
    <property name="Destination" column="Destination" />
    <property name="FromDate" column="FromDate" />
    <property name="TillDate" column="TillDate"  />
    <property name="Duration" column="Duration"  />
    <property name="Rate" column="Rate" />
    <property name="RateType" column="RateType" />
    <property name="Amount" column="Amount" />
    <property name="NumberOfCalls" column="NumberOfCalls" />
    <property name="MonthNumber" column="MonthNumber" />
    <property name="YearNumber" column="YearNumber" />
    <many-to-one name="Billing_Invoice" column="InvoiceID" fetch="select"/>
    <many-to-one name="Currency" column="CurrencyID" fetch="select" />
    <many-to-one name="User" column="UserID" fetch="select" />
  </class>
</hibernate-mapping>
