<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
	<class name="PlaningRate" table="PlaningRate" dynamic-update="false" lazy="false">
		<!-- <cache usage="nonstrict-read-write"/> -->
		<id name="ID" column="PlanningRateID" unsaved-value="0">
			<generator class="identity" />
		</id>
		<many-to-one name="RatePlan" column="RatePlanID" fetch="select" />		
		<many-to-one name="Zone" column="ZoneID" fetch="select" />
		<property name="ServicesFlag" column="ServicesFlag" />
		<property name="Value" column="Rate" type="Decimal" />
		<property name="OffPeakRate" column="OffPeakRate" type="Decimal" />
		<property name="WeekendRate" column="WeekendRate" type="Decimal" />	
		<property name="BeginEffectiveDate" column="BeginEffectiveDate" />
    <property name="EndEffectiveDate" column="EndEffectiveDate" />
    <property name="Notes" column="Notes" />
    <many-to-one name="User" column="UserID" fetch="select" />
	</class>
</hibernate-mapping>