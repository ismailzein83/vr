<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
  <class name="CarrierAccount" table="CarrierAccount" dynamic-update="false" lazy="false" >
    <cache usage="nonstrict-read-write"/>
    <id name="_CarrierAccountID" column="CarrierAccountID" access="field" type="String" length="10">
      <generator class="assigned"/>
    </id>
    <many-to-one name="CarrierProfile" class="CarrierProfile" column="ProfileID" not-null="true" fetch="select" />
    <many-to-one name="CarrierGroup" class="CarrierGroup" column="CarrierGroupID" fetch="select" />
    <property name="ServicesFlag" column="ServicesFlag" />
    <property name="ActivationStatus" column="ActivationStatus" />
    <property name="RoutingStatus" column="RoutingStatus" />
    <property name="AccountType" column="AccountType" />
    <property name="CustomerPaymentType" column="CustomerPaymentType" />
    <property name="SupplierPaymentType" column="SupplierPaymentType" /> 
    <property name="CustomerCreditLimit" column="CustomerCreditLimit" />
    <property name="SupplierCreditLimit" column="SupplierCreditLimit" />
    <property name="SupplierGMTTime" column="GMTTime" />
    <property name="CustomerGMTTime" column="CustomerGMTTime" />
    <property name="BankReferences" column="BankReferences" />
    <property name="Notes" column="Notes" />
    <property name="NominalCapacityInE1s" column="NominalCapacityInE1s"/>
    <property name="RateIncreaseDays" column="RateIncreaseDays"/>
    <property name="SupplierRatePolicy" column="SupplierRatePolicy" />
    <property name="IsOriginatingZonesEnabled" column="IsOriginatingZonesEnabled" type="YesNo" />
    <property name="IsPassThroughCustomer" column="IsPassThroughCustomer" type="YesNo" />
    <property name="IsPassThroughSupplier" column="IsPassThroughSupplier" type="YesNo" />
    <property name="RepresentsASwitch" column="RepresentsASwitch" type="YesNo" />
    <property name="_IsTOD" column="IsTOD" type="String" length="1" access="field" />
    <property name="_IsAToZ" column="IsAToZ" type="String" length="1" access="field" />
    <property name="_IsDeleted" column="IsDeleted" type="String" length="1" access="field" />
    <property name="NameSuffix" column="NameSuffix"></property>
    <property name="ServicesAmount" column="Services" />
    <property name="ConnectionFees" column="ConnectionFees" />
    <property name="ImportEmail" column="ImportEmail"></property>
    <property name="ImportSubjectCode" column="ImportSubjectCode"></property>
    <property name="IsNettingEnabled" column="IsNettingEnabled" type="YesNo"/>

    <property name="CustomerSMSOnPayment" column="CustomerSMSOnPayment" type="YesNo"/>
    <property name="CustomerMailOnPayment" column="CustomerMailOnPayment" type="YesNo"/>
    <property name="SupplierSMSOnPayment" column="SupplierSMSOnPayment" type="YesNo"/>
    <property name="SupplierMailOnPayment" column="SupplierMailOnPayment" type="YesNo"/>

    <property name="CustomerAllowPayment" column="CustomerAllowPayment" type="YesNo"/>
    <property name="SupplierAllowPayment" column="SupplierAllowPayment" type="YesNo"/>
    
    <property name="CustomerActivateDate" column="CustomerActivateDate" />
    <property name="CustomerDeactivateDate" column="CustomerDeactivateDate" />
    <property name="SupplierActivateDate" column="SupplierActivateDate" />
    <property name="SupplierDeactivateDate" column="SupplierDeactivateDate" />
    <property name="InvoiceSerialPattern" column="InvoiceSerialPattern" />
    <property name="CustomerMask" column="CustomerMask" />
    <property name="IsCustomerCeiling" column="IsCustomerCeiling"/>
    <property name="IsSupplierCeiling" column="IsSupplierCeiling"/>
    <property name="CarrierGroups" column="CarrierGroups"/>
    <property name="IsCustomCodeView" column="IsCustomCodeView" type="YesNo"/>
    <property name="CodeView" column="CodeView"/>
    <property name="CarrierMask" column="CarrierMask"/>
    <property name="IsProduct" column="IsProduct"/>
    <property name="_IsCustomDispute" column="IsCustomDispute" type="String" length="1" access="field"/>
    <set name="Connections" inverse="true" lazy="false" fetch="select" >
      <key column="CarrierAccountID" />
      <one-to-many class="CarrierAccountConnection" />
    </set>
    <many-to-one name="User" column="UserID" fetch="select" />
    <!--<many-to-one name="Setting" class="AutoInvoiceSetting" column="AutomaticInvoiceSettingID" fetch="select" />-->

  </class>
</hibernate-mapping>