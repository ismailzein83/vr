<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
	<class name="PricelistImportOption" table="PricelistImportOption" dynamic-update="false" lazy="false">
		<id name="Supplier" column="SupplierID">
			<generator class="assigned" />
		</id>
		<property name="ImporterName" column="ImporterName" />
		<property name="ImportParametersString" column="ImportParameters"  type="StringClob" access="property"/>
		<property name="LastUpdate" column="LastUpdate" />
		<many-to-one name="User" column="UserID" fetch="select" />
	</class>
</hibernate-mapping>