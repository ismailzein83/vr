<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
  <class name="RouteOverride" table="RouteOverride" dynamic-update="false" lazy="false" >
    <!-- <cache usage="nonstrict-read-write"/> -->
    <composite-id name="ID" class="RouteOverrideCompositeID">
      <key-property name="Customer" column="CustomerID" />
      <key-property name="Code" column="Code" />
      <key-property name="OurZone" column="OurZoneID" />
    </composite-id>
    
    <property name="IncludeSubCodes" column="IncludeSubCodes" type="YesNo" />
	  <property name="_RouteOptions" column="RouteOptions" />
	  <property name="_BlockedSuppliers" column ="BlockedSuppliers" />
    <property name="ExcludedCodes" column ="ExcludedCodes" />
    <property name="BeginEffectiveDate" column="BeginEffectiveDate" />
    <property name="EndEffectiveDate" column="EndEffectiveDate" />
    <property name="Reason" column="Reason" />
    <property name="Updated" column="Updated" />
    <many-to-one name="User" column="UserID" fetch="select" />
  </class>
</hibernate-mapping>

