<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
  <class name="PriceListChangeLog" table="PriceListChangeLog" discriminator-value="U" lazy="false">
    <id name="ID" column="LogID">
      <generator class="identity" />
    </id>
    <discriminator column="ObjectType" type="String" />
    <property name="Date" column="Date" type="DateTime"></property>
    <many-to-one name="PriceList" column="PriceListID" not-null="true"></many-to-one>
    <property name="ChangeType" column="ChangeType"></property>
    <subclass name="PriceListZoneChangeLog" discriminator-value="Z" lazy="false">
      <many-to-one name="Zone" column="ObjectID" not-null="true"></many-to-one>
    </subclass>
    <subclass name="PriceListCodeChangeLog" discriminator-value="C" lazy="false">
      <many-to-one name="Code" column="ObjectID" not-null="true"></many-to-one>
    </subclass>
    <subclass name="PriceListRateChangeLog" discriminator-value="R" lazy="false">
      <many-to-one name="Rate" column="ObjectID" not-null="true"></many-to-one>
    </subclass>
  </class>
</hibernate-mapping>