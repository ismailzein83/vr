<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
	<class name="Alert" table="Alert" dynamic-update="false" lazy="false" >
		<!-- <cache usage="nonstrict-read-write"/> -->
		<id name="ID" column="ID">
			<generator class="identity" />
		</id>
		<property name="Created" column="Created" />
		<property name="Source" column="[Source]"/>
		<property name="Description" column="[Description]" />
		<property name="Level" column="[Level]" />
		<property name="Progress" column="[Progress]" />
		<property name="Tag" column="[Tag]" />
		<property name="IsVisible" column="IsVisible" type="YesNo" />
	</class>
</hibernate-mapping>