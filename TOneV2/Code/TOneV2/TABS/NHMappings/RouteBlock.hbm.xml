<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
	<class name="RouteBlock" table="RouteBlock" dynamic-update="false" lazy="false">
		<!-- <cache usage="nonstrict-read-write"/> -->
		<id name="RouteBlockID" column="RouteBlockID" type="Int32" unsaved-value="0">
			<generator class="identity" />
		</id>
		<many-to-one name="Customer" column="CustomerID" />
		<many-to-one name="Supplier" column="SupplierID" />
		<many-to-one name="Zone" column="ZoneID"  fetch="select" />
		<property name="Code" column="Code" />
    <property name="UpdateDate" column="UpdateDate" type="DateTime" />
		<property name="BeginEffectiveDate" column="BeginEffectiveDate" type="DateTime" />
		<property name="EndEffectiveDate" column="EndEffectiveDate" type="DateTime" />
    <property name="BlockType" column="BlockType" />
    <property name="ExcludedCodes" column ="ExcludedCodes" />
    <property name="Reason" column="Reason" />
    <property name="IncludeSubCodes" column="IncludeSubCodes" type="YesNo" />
    
    <many-to-one name="User" column="UserID" fetch="select" />
    <many-to-one name="RouteChangeHeader" column="RouteChangeHeaderID" fetch="select" />
	</class>
</hibernate-mapping>