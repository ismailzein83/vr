<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
  <class name="Commission" table="Commission" dynamic-update="false" lazy="false">
    <!-- <cache usage="nonstrict-read-write"/> -->
    <id name="CommissionID" column="CommissionID" unsaved-value="0">
      <generator class="identity" />
    </id>
    <many-to-one name="Supplier" column="SupplierID" />
    <many-to-one name="Customer" column="CustomerID" />
    <many-to-one name="Zone" column="ZoneID" />
	  <property name="FromRate" column="FromRate" />
	  <property name="ToRate" column="ToRate" />
	  <property name="Percentage" column ="Percentage" />
    <property name="Amount" column="Amount" />
    <property name="BeginEffectiveDate" column="BeginEffectiveDate" />
    <property name="EndEffectiveDate" column="EndEffectiveDate" />
	  <property name="IsExtraCharge" column="IsExtracharge" type="YesNo" />	
    <many-to-one name="User" column="UserID" fetch="select" />
  </class>
</hibernate-mapping>

