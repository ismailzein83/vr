<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
  <class name="PrepaidAmount" table="PrepaidAmount" dynamic-update="false" lazy="false" >
    <id name="ID" column="ID">
      <generator class="identity"/>
    </id>
    <property name="Amount" column="Amount" />
    <property name="Date" column="Date" />
    <property name="Type" column="Type" />
    <property name="LastUpdate" column="LastUpdate" />
    <property name="Tag" column="Tag" />
    <property name="ReferenceNumber" column="ReferenceNumber"/>
    <property name="Note" column="Note"/>
    <many-to-one name="SupplierProfile" column="SupplierProfileID" fetch="select"/>
    <many-to-one name="CustomerProfile" column="CustomerProfileID" fetch="select"/>
    <many-to-one name="Supplier" column="SupplierID" fetch="select"/>
    <many-to-one name="Customer" column="CustomerID" fetch="select"/>
    <many-to-one name="Currency" column="CurrencyID" fetch="select" />
    <many-to-one name="User" column="UserID" fetch="select" />
  </class>
</hibernate-mapping>