<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
  <class name="RatePlan" table="RatePlan" dynamic-update="false" lazy="false">
	  <!-- <cache usage="nonstrict-read-write"/> -->
    <id name="ID" column="RatePlanID" type="Int32" unsaved-value="0">
      <generator class="identity" />
    </id>
    <many-to-one name="Customer" column="CustomerID" fetch="select" />
    <property name="Description" column="Description" type="String" />
    <many-to-one name="Currency" column="CurrencyID" fetch="select" />
    <property name="BeginEffectiveDate" column="BeginEffectiveDate" type="DateTime" />
    <many-to-one name="User" column="UserID" fetch="select" />
  </class>
</hibernate-mapping>