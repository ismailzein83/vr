<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
	<class name="RouteChangeHeader" table="RouteChangeHeader" dynamic-update="false" lazy="false" >
		<!-- <cache usage="nonstrict-read-write"/> -->
		<id name="ID" column="ID">
			<generator class="identity" />
		</id>
		<property name="Reason" column="Reason"/>
		<property name="Created" column="Created" />
		<property name="IsEnded" column="IsEnded" type="YesNo" />
		<many-to-one name="User" column="UserID" fetch="select" />
	</class>
</hibernate-mapping>