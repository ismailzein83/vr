<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
	<class name="FlaggedService" table="FlaggedService" mutable="false" dynamic-update="false" lazy="false">
		<cache usage="nonstrict-read-write"/>
		<id name="_FlaggedServiceID" column="FlaggedServiceID" access="field">
			<generator class="assigned" />
		</id>
		<property name="Symbol" column="Symbol"  />
		<property name="Name" column="[Name]"  />
		<property name="Description" column="[Description]" />
		<many-to-one name="User" column="UserID" fetch="select" />
	</class>
</hibernate-mapping>