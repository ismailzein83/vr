        <property name="From" column="[From]" type="String" />
        <property name="To" column="[To]" type="String" />
        <property name="Body" column="Body" type="String" />
        <property name="DeliveryDate" column="DeliveryDate" type="DateTime" />
        <property name="CC" column="CC" type="String" />
        <property name="BCC" column="BCC" type="String" />
        <property name="Attachment" column="Attachment" />
        <property name="AttachmentFileName" column="AttachmentFileName" type="string" />
        <property name="ContentType" column="ContentType" />
