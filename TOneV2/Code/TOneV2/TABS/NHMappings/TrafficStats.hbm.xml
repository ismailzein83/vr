<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
  <class name="TrafficStats" table="TrafficStats" dynamic-update="false" lazy="false">
    <id name="ID" column="[ID]" unsaved-value="0">
      <generator class="identity" />
    </id>
    <many-to-one name="Switch" column="SwitchID" fetch="select"/>

    <many-to-one name="OurZone" column="OurZoneID" fetch="select"/>
    <many-to-one name="OriginatingZone" column="OriginatingZoneID" fetch="select"/>
    <many-to-one name="SupplierZone" column="SupplierZoneID" fetch="select"/>
    <many-to-one name="Supplier" column="SupplierID" fetch="select"/>
    <many-to-one name="Customer" column="CustomerID" fetch="select"/>

    <property name="Port_IN" column="Port_IN"/>
    <property name="Port_OUT" column="Port_OUT"/>
    <property name="FirstCDRAttempt" column="FirstCDRAttempt"/>
    <property name="LastCDRAttempt" column="LastCDRAttempt"/>
    <property name="Attempts" column="Attempts"/>
    <property name="DeliveredAttempts" column="DeliveredAttempts"/>
    <property name="SuccessfulAttempts"	column="SuccessfulAttempts"/>
    <property name="DurationsInSeconds"	column="DurationsInSeconds"/>
    <property name="PDDInSeconds" column="PDDInSeconds"/>
    <property name="UtilizationInSeconds" column="UtilizationInSeconds"/>
    <property name="NumberOfCalls" column="NumberOfCalls"/>
    <property name="DeliveredNumberOfCalls" column="DeliveredNumberOfCalls"/>
    <property name="MaxDurationInSeconds" column="MaxDurationInSeconds"/>
    <property name="PGAD" column="PGAD"/>
    <property name="CeiledDuration" column="CeiledDuration"/>
    <property name="ReleaseSourceAParty" column="ReleaseSourceAParty"/>
  </class>
</hibernate-mapping>
