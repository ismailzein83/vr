<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
	<class name="Currency" table="Currency" dynamic-update="false" lazy="false">
		<cache usage="nonstrict-read-write"/>
		<id name="_Symbol" column="CurrencyID" type="string" length="3" unsaved-value="" access="field">
			<generator class="assigned" />
		</id>
		<property name="_Name" column="Name" type="String" length="100" access="field" />
		<property name="_IsMainCurrency" column="IsMainCurrency" type="String" length="1" access="field" />
		<property name="_IsVisible" column="IsVisible" type="String" length="1" access="field"/>
		<property name="LastRate" column="LastRate" />
		<property name="LastUpdated" column="LastUpdated" />
		<many-to-one name="User" column="UserID" fetch="select" />
	</class>
</hibernate-mapping>
