<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
	<class name="Billing_CDR_Cost" table="Billing_CDR_Cost" >

		<id name="ID" column="[ID]">
			<generator class="foreign">
				<param name="property">Billing_CDR_Main</param>
			</generator>
		</id>

		<property name="Net" column="[Net]" />

		<property name="RateValue" column="RateValue"  />
		<property name="Discount" column="Discount"  />
		<property name="RateType" column="RateType" />
		<property name="FirstPeriod" column="FirstPeriod"  />
		<property name="RepeatFirstperiod" column="RepeatFirstperiod" />
		<property name="FractionUnit" column="FractionUnit" />
		<property name="CommissionValue" column="CommissionValue" />
		<property name="ExtraChargeValue" column="ExtraChargeValue"  />
		<property name="Updated" column="Updated"  />
    <property name="Attempt" column="Attempt"  />
    <property name="DurationInSeconds" column="DurationInSeconds" />
    <property name="Code" column="Code" />
		<one-to-one name="Billing_CDR_Main" constrained="true" fetch="join" cascade="none" />

		<many-to-one name="Zone" column="ZoneID" fetch="select" />
		<many-to-one name="Currency" column="CurrencyID" fetch="select" />
		<many-to-one name="Tariff" column="TariffID" fetch="select" />
		<many-to-one name="Rate" column="RateID" fetch="select" />
		<many-to-one name="ToDConsideration" column="ToDConsiderationID" fetch="select" />
		<many-to-one name="Commission" column="CommissionID" fetch="select" />
		<many-to-one name="ExtraCharge" column="ExtraChargeID" fetch="select" />

	</class>
</hibernate-mapping>
