<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
	<class name="SystemParameter" table="SystemParameter" dynamic-update="false" lazy="false" >
		<cache usage="nonstrict-read-write"/>
		<id name="Name" column="[Name]" unsaved-value="null">
			<generator class="assigned" />
		</id>
		<property name="Type" column="[Type]" />
		<property name="NumericValue" column="NumericValue" />
		<property name="_TimeSpanValue" column="TimeSpanValue" access="field" />
		<property name="DateTimeValue" column="DateTimeValue" />
		<property name="BooleanValue" column="BooleanValue" type="YesNo" />
		<property name="TextValue" column="TextValue" />
		<property name="LongTextValue" column="LongTextValue" type="StringClob" access="property" />
		<property name="Description" column="[Description]"  type="StringClob" access="property"/>
		<many-to-one name="User" column="UserID" fetch="select" />
	</class>
</hibernate-mapping>
