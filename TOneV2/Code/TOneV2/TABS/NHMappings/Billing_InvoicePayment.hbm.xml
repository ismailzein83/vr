<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
  <class name="Billing_InvoicePayment" table="Billing_InvoicePayment"  dynamic-update="false" lazy="false" >
    <id name="InvoicePaymentID"  column="InvoicePaymentID" unsaved-value="0" type="Int32">
      <generator class="identity"/>
    </id>
    <property name="DueDate" column="DueDate" />
    <property name="Amount" column="Amount" />
    <property name="PaidDate" column="PaidDate" />
    <many-to-one name="Billing_Invoice" column="InvoiceID" fetch="select" />
  </class>
</hibernate-mapping>
