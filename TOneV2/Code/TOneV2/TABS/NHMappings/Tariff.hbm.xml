<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
	<class name="Tariff" table="Tariff" dynamic-update="false" lazy="false">
		<!-- <cache usage="nonstrict-read-write"/> -->
		<id name="TariffID" column="TariffID" unsaved-value="0">
			<generator class="identity" />
		</id>
		<many-to-one name="Zone" column="ZoneID" fetch="select" />
		<many-to-one name="Supplier" column="SupplierID" fetch="select" />
		<many-to-one name="Customer" column="CustomerID" fetch="select" />
		<property name="CallFee" column="CallFee" />
		<property name="FirstPeriodRate" column="FirstPeriodRate"  />
		<property name="FirstPeriod" column="FirstPeriod" />
		<property name="RepeatFirstPeriod" column="RepeatFirstPeriod" type="YesNo" />
		<property name="FractionUnit" column="FractionUnit" />
		<property name="BeginEffectiveDate" column="BeginEffectiveDate" />
		<property name="EndEffectiveDate" column="EndEffectiveDate" />
		<many-to-one name="User" column="UserID" fetch="select" />	
	</class>
</hibernate-mapping>
