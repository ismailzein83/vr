<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
	<class name="PriceList" table="PriceList" dynamic-update="false" lazy="false" >
		<!-- <cache usage="nonstrict-read-write"/> -->
		<id name="ID" column="PriceListID" type="Int32" unsaved-value="0">
			<generator class="identity" />
		</id>
		<many-to-one name="Supplier" column="SupplierID" fetch="select" />
		<many-to-one name="Customer" column="CustomerID" fetch="select" />
		<property name="Description" column="Description" type="String" />
		<many-to-one name="Currency" column="CurrencyID" fetch="select" />
		<property name="BeginEffectiveDate" column="BeginEffectiveDate" type="DateTime" />
		<property name="EndEffectiveDate" column="EndEffectiveDate" type="DateTime" />
    <property name="IsSent" column="IsSent" type="YesNo"/>
		<property name="SourceFileName" column="SourceFileName" />
		<many-to-one name="User" column="UserID" fetch="select" />
	</class>
</hibernate-mapping>