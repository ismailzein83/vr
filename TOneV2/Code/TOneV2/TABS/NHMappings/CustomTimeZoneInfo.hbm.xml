<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
	<class name="CustomTimeZoneInfo" table="CustomTimeZoneInfo" dynamic-update="false" lazy="false">
		<id name="ID" column="ID" unsaved-value="0" >
			<generator class="identity" />
		</id>
		<property name="BaseUtcOffset" column="BaseUtcOffset" />
    <property name="DisplayName" column="DisplayName" />
    <many-to-one name="User" column="UserID" fetch="select" />
	</class>
</hibernate-mapping>
