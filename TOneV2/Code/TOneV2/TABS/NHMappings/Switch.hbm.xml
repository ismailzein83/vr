<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TABS" namespace="TABS">
	<class name="Switch" table="Switch" dynamic-update="false" lazy="false" >
		<cache usage="nonstrict-read-write"/>
		<id name="SwitchID" column="SwitchID" type="Int32" unsaved-value="0">
			<generator class="identity" />
		</id>
		<property name="Symbol" column="Symbol" />
		<property name="Name" column="Name" />
		<property name="Description" column="Description" />
		<property name="LastCDRImportTag" column="LastCDRImportTag" update="false" insert="false" />
    <property name="LastImport" column="LastImport" />
    <property name="LastAttempt" column="LastAttempt" />
    <property name="NominalTrunkCapacityInE1s" column="NominalTrunkCapacityInE1s" />
    <property name="NominalVoipCapacityInE1s" column="NominalVoipCapacityInE1s" />
    <property name="LastRouteUpdate" column="LastRouteUpdate" />
    <property name="Enable_CDR_Import" column="Enable_CDR_Import" type="YesNo" />
    <property name="Enable_Routing" column="Enable_Routing" type="YesNo" />
    <property name="ConfigurationXmlString" column="[Configuration]"  type="StringClob" access="property" />
    <many-to-one name="User" column="UserID" fetch="select" />		
    <map name="ReleaseCodes" lazy="false" cascade="delete" fetch="select">
			<key column="SwitchID" />
			<index column="ReleaseCode" type="string" />
			<one-to-many class="SwitchReleaseCode" />
		</map>
	</class>
</hibernate-mapping>
