<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="vanriseRuntime" type="Vanrise.Runtime.Configuration.RuntimeConfig, Vanrise.Runtime" />
    <section name="vanriseLogging" type="Vanrise.Common.LoggingConfiguration.LoggingConfig, Vanrise.Common" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>

  <vanriseRuntime>
    <runtimeServiceGroups>
      <runtimeServiceGroup name="group1" nbOfRuntimeInstances="2">
        <runtimeServices>
          <runtimeService name="Business Process Service" type="Vanrise.BusinessProcess.BusinessProcessService, Vanrise.BusinessProcess" interval="00:00:02"></runtimeService>
        </runtimeServices>
      </runtimeServiceGroup>
      <!--<runtimeServiceGroup name="group2" nbOfRuntimeInstances="1">
        <runtimeServices>
          <runtimeService name="Scheduler Service" type="Vanrise.Runtime.SchedulerService, Vanrise.Runtime" interval="00:00:02"></runtimeService>
        </runtimeServices>
      </runtimeServiceGroup>
      <runtimeServiceGroup name="group3" nbOfRuntimeInstances="15">
        <runtimeServices>
          <runtimeService name="Queue Activation Service" type="Vanrise.Queueing.QueueActivationService, Vanrise.Queueing" interval="00:00:02"></runtimeService>
        </runtimeServices>
      </runtimeServiceGroup>
      <runtimeServiceGroup name="group4" nbOfRuntimeInstances="12">
        <runtimeServices>
          <runtimeService name="Data Source Runtime Service" type="Vanrise.Integration.Business.DataSourceRuntimeService, Vanrise.Integration.Business" interval="00:00:02"></runtimeService>
        </runtimeServices>
      </runtimeServiceGroup>-->
    </runtimeServiceGroups>
  </vanriseRuntime>
  
  <connectionStrings>
    <add name="TransactionDBConnString"    connectionString="Integrated Security=SSPI;Persist Security Info=False;Initial Catalog=FZeroTransaction;Data Source=192.168.25.11"/>
    <add name="QueueDBConnString"          connectionString="Initial Catalog=FZeroQueue;Data Source=.;User ID=sa;Password=p@ssw0rd"/>
    <add name="LogDBConnString"            connectionString="Integrated Security=SSPI;Persist Security Info=False;Initial Catalog=FZeroLog;Data Source=192.168.25.11"/>
    <add name="CDRDBConnectionString"      connectionString="Data Source=192.168.110.185;Initial Catalog=CDRAnalysisMobile_WF;User ID=development;Password=dev!123;"            providerName="System.Data.SqlClient" />
    <add name="ConfigurationDBConnString"      connectionString="Data Source=192.168.110.185;Initial Catalog=CDRAnalysisConfiguration;User ID=development;Password=dev!123;"            providerName="System.Data.SqlClient" />
    <add name="DWSDBConnString"      connectionString="Data Source=192.168.110.185;Initial Catalog=CDRAnalysisDW;User ID=development;Password=dev!123;"            providerName="System.Data.SqlClient" />
    <add name="FraudAnalysis_CDRConnStringTemplateKey" connectionString="Initial Catalog=CDRAnalysisDev_CDR_{0};Data Source=192.168.110.185;User ID=development;Password=dev!123;"/>
  </connectionStrings>

  <appSettings>
    <add key="OperatorType" value="1" />    <!--  1 =Mobile ,  2 = PSTN -->
    <add key="RulesDBConnStringKey" value="ConfigurationDBConnString"/>
    <add key="SecurityDBConnStringKey" value="ConfigurationDBConnString"/>
    <add key="DWFactBatchSize" value="100000" />
    <add key="RuntimeConnStringKey" value="TransactionDBConnString"/>
    <add key="BusinessProcessDBConnStringKey" value="TransactionDBConnString"/>
    <add key="QueueDBConnStringKey" value="TransactionDBConnString"/>
    <add key="QueueItemDBConnStringKey" value="QueueDBConnString"/>
    <add key="BusinessProcessTrackingDBConnStringKey" value="LogDBConnString"/>
    <add key="AddBusinessProcessConsoleTracking" value="true"/>
    <add key="IntegrationConnStringKey" value="TransactionDBConnString"/>
    <add key="BCPDonotDeleteFiles" value="false"/>    <!--this should be always false at production. could be only set to true for troubleshooting-->
    <add key="BCPTempFilesDirectory" value="C:\BCPFiles" />
    <add key="VanriseCache_CleanMemoryThreshold" value="999999999999999"/>    
    <add key="FraudAnalysis_CDRNormalPrefixLength" value="10"/>


    <add key="IsRuntimeService" value="false"/>
  </appSettings>

  <vanriseLogging maxLogLevel="Information">
    <loggers>
      <logger name="SQLLogger" type="Vanrise.Logging.SQL.SQLLogger, Vanrise.Logging.SQL">
        <parameters>
          <parameter name="ConnectionStringKey" value="LogDBConnString" />
        </parameters>
      </logger>
      <logger name="ConsoleLogger"  type="Vanrise.Common.ConsoleLogger, Vanrise.Common"></logger>
    </loggers>
    <exceptionLoggers>
      <exceptionLogger name="ConsoleExceptionLogger"  type="Vanrise.Common.ConsoleExceptionLogger, Vanrise.Common"></exceptionLogger>
      <exceptionLogger name="DefaultExceptionLogger"  type="Vanrise.Common.DefaultExceptionLogger, Vanrise.Common"></exceptionLogger>
    </exceptionLoggers>
  </vanriseLogging>

</configuration>