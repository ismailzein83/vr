<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="vanriseLogging" type="Vanrise.Common.LoggingConfiguration.LoggingConfig, Vanrise.Common" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <connectionStrings>
    <add name="TransactionDBConnString"    connectionString="Integrated Security=SSPI;Persist Security Info=False;Initial Catalog=FZeroTransaction;Data Source=."/>
    <add name="QueueDBConnString"          connectionString="Integrated Security=SSPI;Persist Security Info=False;Initial Catalog=FZeroQueue;Data Source=."/>
    <add name="LogDBConnString"            connectionString="Integrated Security=SSPI;Persist Security Info=False;Initial Catalog=FZeroLog;Data Source=."/>
    <add name="CDRDBConnectionString"      connectionString="Data Source=192.168.110.185;Initial Catalog=CDRAnalysisMobile_WF;User ID=development;Password=dev!123;"            providerName="System.Data.SqlClient" />
    <add name="ConfigurationDBConnString"      connectionString="Data Source=192.168.110.185;Initial Catalog=CDRAnalysisConfiguration;User ID=development;Password=dev!123;"            providerName="System.Data.SqlClient" />
  </connectionStrings>

  <appSettings>
    <add key="NumberProfileBatchSize" value="100000" />
    <add key="SERVER" value="192.168.110.241" />
    <add key="Server_Username" value="root" />
    <add key="Server_Pasword" value="P@ssw0rd" />
    <add key="SFTP_Dir" value="/var/mysql/5.5/data"/>
    <add key="RuntimeConnStringKey" value="TransactionDBConnString"/>
    <add key="BusinessProcessDBConnStringKey" value="TransactionDBConnString"/>
    <add key="QueueDBConnStringKey" value="TransactionDBConnString"/>
    <add key="QueueItemDBConnStringKey" value="QueueDBConnString"/>
    <add key="BusinessProcessTrackingDBConnStringKey" value="LogDBConnString"/>
    <add key="AddBusinessProcessConsoleTracking" value="true"/>
    <add key="IntegrationConnStringKey" value="TransactionDBConnString"/>
    <add key="OperatorType" value="1"/>   <!--1 = Mobile, 2 =PSTN -->
    <add key="LoadCDRsDirectory" value="C:\CDRAnalysis_LoadCDRs"/>
    <add key="IsRuntimeService" value="false"/>
    <add key="BCPDonotDeleteFiles" value="true"/>
    <add key="BCPTempFilesDirectory" value="C:\BCPFiles" />

  </appSettings>

  <vanriseLogging maxLogLevel="Information">
    <loggers>
      <logger name="SQLLogger" type="Vanrise.Logging.SQL.SQLLogger, Vanrise.Logging.SQL">
        <parameters>
          <parameter name="ConnectionStringKey" value="LogDBConnString" />
        </parameters>
      </logger>
      <logger name="ConsoleLogger"  type="Vanrise.Common.ConsoleLogger, Vanrise.Common"></logger>
    </loggers>
    <exceptionLoggers>
      <exceptionLogger name="ConsoleExceptionLogger"  type="Vanrise.Common.ConsoleExceptionLogger, Vanrise.Common"></exceptionLogger>
    </exceptionLoggers>
  </vanriseLogging>
  
</configuration>